<?xml version="1.0"?>
<!-- Starts the node that turns on/off RTAB-Map and the node that republishes tf as odometry messages-->
<launch>
    <node pkg="env_mapping" type="env_mapping" name="env_mapping" output="screen"/>
    <node pkg="env_mapping" type="concatenatePC" name="concatenatePC" output="screen"/>
    <node pkg="env_mapping" type="map_odometry" name="map_odometry" output="screen"/>
</launch>
